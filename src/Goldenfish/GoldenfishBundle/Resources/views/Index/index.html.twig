                <div class="row">
                    <div class="col-md-1"></div>
                    <div class="col-md-10 wow fadeIn">
                        <h1 class="text-center">La Team</h1>
                        <div class="col-md-12">
                            <h2 class="text-center">Chef de laboratoire</h2>
                            <div class="col-md-4"></div>`
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/marjo.png"  class="img-circle">
                            </div>
                            <div class="col-md-4"></div>
                        </div>
                        <!-- chef de projet -->
                        <div class="col-md-12">
                            <h2 class="text-center">Chef de Projet</h2>
                            <div class="col-md-4"></div>`
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/Etienne.jpg"  class="img-circle">
                            </div>
                            <div class="col-md-4"></div>
                        </div>
                        <!-- Dev -->
                        <div class="col-md-12">
                            <h2 class="text-center">Développeurs</h2>
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/benjirouq.png" class="img-circle">
                            </div>
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/champo.png" class="img-circle">
                            </div>
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/esra.jpg" class="img-circle">
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="col-md-2"></div>
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/thibo.jpg" class="img-circle">
                            </div>
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/Loic.png" class="img-circle">
                            </div>
                            <div class="col-md-2"></div>
                        </div>
                        <!-- Escca -->
                        <div class="col-md-12">
                            <h2 class="text-center">Commerciaux</h2>
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/anais.jpg" class="img-circle">
                            </div>
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/remana.jpg" class="img-circle">
                            </div>
                            <div class="col-md-4">
                                <img src="../bundles/goldenfish/image/esra.jpg" class="img-circle">
                            </div>
                        </div>
                    </div>
                    <div class="col-md-1"></div>
                </div>
            </div>
        </div>
    </section>
    <br/>
    <br/>

    <!-- Contact -->
    <section id="contact">
        <div class="overlay">
            <div class="container">
                <div class="row">
                    <div class="col-md-1"></div>
                    <div class="col-md-10 wow fadeIn">
                        <h2 class="text-uppercase">Contactez-nous</h2>
                        <form action="#" method="post">
                            <div class="col-md-6">
                                <input type="text" class="form-control" placeholder="Nom">
                            </div>
                            <div class="col-md-6">
                                <input type="email" class="form-control" placeholder="Email">
                            </div>
                            <div class="col-md-12">
                                <input type="text" class="form-control" placeholder="Sujet">
                            </div>
                            <div class="col-md-12">
                                <textarea class="form-control" placeholder="Message" rows="4"></textarea>
                            </div>
                            <div class="col-md-8">
                                <input type="submit" class="form-control text-uppercase" value="Send">
                            </div>
                        </form>
                    </div>
                    <div class="col-md-1"></div>
                </div>
            </div>
        </div>
    </section>
</div>
