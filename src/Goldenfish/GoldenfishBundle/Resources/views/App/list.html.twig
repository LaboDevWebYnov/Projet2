
<link rel="stylesheet" href="../bundles/goldenfish/css/note.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
{% block GFN_body %}

<h1>Mes notes</h1>
{% for note in listNote %}
	<div class="notes">
		<p>{{ note.title }}</p>
		<p>{{ note.dateCreation|date('d/m/Y') }}</p>
		<a href="{{ path('partage_note', {'id': note.id}) }}">Partager</a>
	</div>
{% endfor %}
<h1>Notes partag√©es avec moi</h1>
{% for notepartage in listNotePartage %}
	<div class="notes">
		<p>{{ notepartage.title }}</p>
		<p>{{ notepartage.dateCreation|date('d/m/Y') }}</p>
	</div>
{% endfor %}
        <script type="text/javascript" src="../bundles/goldenfish/js/jquery.js"></script>
{% endblock %}